package token

import "core:fmt"
import "core:log"
import "core:strings"

Ticker :: struct {
    symbol: string,
    priceChange: f64,
    priceChangePercent: f64,
    weightedAvgPrice: f64,
    openPrice: f64,
    highPrice: f64,
    lowPrice: f64,
    lastPrice: f64,
    volume: f64,
    quoteVolume: f64,
    openTime: i64,
    closeTime: i64,
    firstId: i64,
    lastId: i64,
    count: i64,
}

